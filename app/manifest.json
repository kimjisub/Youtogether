{
	"name": "Youtogether",
	"version": "1.0",
	"description": "Watch Youtube Together!",
	"manifest_version": 2,
	"browser_action": {
		"default_popup": "popup.html",
		"default_title": "Youtogether"
	},
	"permissions": ["activeTab", "tabs", "http://*/", "https://*/"],
	"background": {
		"page": "background.html"
	},
	"content_security_policy": "script-src 'self' 'unsafe-eval' https://www.gstatic.com 'sha256-NdF6luvUZMQNvBpR4l3b7feFlAa949czaMnrgfbSXW4='; object-src 'self'"
}
